STK.register("pl.summary.realtimehot",function(a){return function(b){var c={};var d;var e={initPageRecord:function(){var c={};var d=a.sizzle('[action-type="realtimehot_ad"]',b);if(d[0]){c.word=d[0].getAttribute("word");c.url_show=d[0].getAttribute("url_show");c.url2_show=d[0].getAttribute("url2_show");var e=new Image;e.src="//j.s.weibo.com/doit.png"+"?type=show&position=s_bandrealtime_show&word="+encodeURIComponent(c.word)+"&time="+ +(new Date);if(c.url_show){var f=new Image;f.src=decodeURIComponent(c.url_show).replace("__REQUESTID__","s_bandrealtime_show"+ +(new Date)).replace("[timestamp]",+(new Date))}if(c.url2_show){var g=new Image;g.src=decodeURIComponent(c.url2_show).replace("__REQUESTID__","s_bandrealtime_show"+ +(new Date)).replace("[timestamp]",+(new Date))}}},clickRecord:function(a){window.open(a.href_to);var b=new Image;b.src="//j.s.weibo.com/doit.png?type=click&position=s_bandrealtime_click&word="+encodeURIComponent(a.word)+"&time="+ +(new Date);if(a.url_click){var c=new Image;c.src=decodeURIComponent(a.url_click).replace("__REQUESTID__","s_bandrealtime_click"+ +(new Date)).replace("[timestamp]",+(new Date))}if(a.url2_click){var d=new Image;d.src=decodeURIComponent(a.url2_click).replace("__REQUESTID__","s_bandrealtime_click"+ +(new Date)).replace("[timestamp]",+(new Date))}}};var f={realtimehotClick:function(b){var c=a.core.json.merge(b.data,{word:b.el.getAttribute("word"),url_show:b.el.getAttribute("url_show"),url_click:b.el.getAttribute("url_click"),url2_show:b.el.getAttribute("url2_show"),url2_click:b.el.getAttribute("url2_click"),href_to:b.el.getAttribute("href_to")});e.clickRecord(c)}};var g=function(){d=a.delegatedEvent(b);d.add("realtimehot_ad","click",f.realtimehotClick)};var h=function(){g();e.initPageRecord()};var i=function(){d.remove("realtimehot_ad","click",f.realtimehotClick)};h();c.destroy=i;return c}});
STK.pageletM.register("pl.summary.index",function(a){var b={};var c=a.E("pl_top_realtimehot");var d=a.pl.summary.realtimehot(c,b);return d});
STK.pageletM.start();
